<h1>Listing tipo_productos</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Descripcion</th>
    <th>Destacado</th>
    <th>Foto</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tipo_productos.each do |tipo_producto| %>
  <tr>
    <td><%= tipo_producto.nombre %></td>
    <td><%= tipo_producto.descripcion %></td>
    <td><%= tipo_producto.destacado %></td>
    <td><%= tipo_producto.foto %></td>
    <td><%= link_to 'Show', tipo_producto %></td>
    <td><%= link_to 'Edit', edit_tipo_producto_path(tipo_producto) %></td>
    <td><%= link_to 'Destroy', tipo_producto, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Tipo producto', new_tipo_producto_path %>
